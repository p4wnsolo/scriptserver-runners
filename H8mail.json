{
  "name": "H8mail",
  "script_path": "docker run -it kh4st3x00/h8mail",
  "working_directory": "~/script-server/scripts",
  "group": "Email",
  "output_format": "terminal",
  "parameters": [
    {
      "name": "Email (-t) or URL (-u)",
      "required": true,
      "same_arg_param": true,
      "type": "list",
      "default": "-t",
      "values": [
        "-t",
        "-u"
      ]
    },
    {
      "name": "Email address/URL/username",
      "same_arg_param": true,
      "type": "text"
    },
    {
      "name": "This is a Username",
      "param": "-q username",
      "type": "text",
      "no_value": true,
      "description": "Check this box to search for a username.  Only use this with -t."
    },
    {
      "name": "Search for breaches",
      "param": "-q domain",
      "type": "text",
      "no_value": true
    }
  ]
}